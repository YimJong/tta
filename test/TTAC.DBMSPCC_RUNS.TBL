DROP TABLE TTAC.DBMSPCC_RUNS CASCADE CONSTRAINTS;

CREATE GLOBAL TEMPORARY TABLE TTAC.DBMSPCC_RUNS
(
  RUN_ID         NUMBER(38),
  RUN_COMMENT    VARCHAR2(4000 BYTE),
  RUN_OWNER      VARCHAR2(128 BYTE) CONSTRAINT DBMSPCC_RUNS_RUN_OWNER_NN NOT NULL,
  RUN_TIMESTAMP  DATE CONSTRAINT DBMSPCC_RUNS_RUN_TIMESTAMP_NN NOT NULL
)
ON COMMIT PRESERVE ROWS
NOCACHE;


CREATE UNIQUE INDEX TTAC.DBMSPCC_RUNS_PK ON TTAC.DBMSPCC_RUNS
(RUN_ID);

ALTER INDEX TTAC.DBMSPCC_RUNS_PK
  MONITORING USAGE;

ALTER TABLE TTAC.DBMSPCC_RUNS ADD (
  CONSTRAINT DBMSPCC_RUNS_PK
  PRIMARY KEY
  (RUN_ID)
  USING INDEX TTAC.DBMSPCC_RUNS_PK
  ENABLE VALIDATE);


CREATE OR REPLACE PUBLIC SYNONYM DBMSPCC_RUNS FOR TTAC.DBMSPCC_RUNS;


GRANT DELETE, INSERT, SELECT, UPDATE ON TTAC.DBMSPCC_RUNS TO PUBLIC;
