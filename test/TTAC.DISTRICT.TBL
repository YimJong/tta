DROP TABLE TTAC.DISTRICT CASCADE CONSTRAINTS;

CREATE TABLE TTAC.DISTRICT
(
  D_ID         NUMBER(2),
  D_W_ID       NUMBER(4),
  D_YTD        NUMBER(12,2),
  D_TAX        NUMBER(4,4),
  D_NEXT_O_ID  NUMBER,
  D_NAME       VARCHAR2(10 BYTE),
  D_STREET_1   VARCHAR2(20 BYTE),
  D_STREET_2   VARCHAR2(20 BYTE),
  D_CITY       VARCHAR2(20 BYTE),
  D_STATE      CHAR(2 BYTE),
  D_ZIP        CHAR(9 BYTE)
)
TABLESPACE USERS
PCTFREE    99
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;


CREATE UNIQUE INDEX TTAC.DISTRICT_I1 ON TTAC.DISTRICT
(D_W_ID, D_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.DISTRICT_I1
  MONITORING USAGE;
