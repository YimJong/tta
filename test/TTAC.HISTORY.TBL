DROP TABLE TTAC.HISTORY CASCADE CONSTRAINTS;

CREATE TABLE TTAC.HISTORY
(
  H_C_ID    NUMBER,
  H_C_D_ID  NUMBER,
  H_C_W_ID  NUMBER,
  H_D_ID    NUMBER,
  H_W_ID    NUMBER,
  H_DATE    DATE,
  H_AMOUNT  NUMBER(6,2),
  H_DATA    VARCHAR2(24 BYTE)
)
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;
