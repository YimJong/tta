DROP TABLE TTAC.ITEM CASCADE CONSTRAINTS;

CREATE TABLE TTAC.ITEM
(
  I_ID     NUMBER(6),
  I_IM_ID  NUMBER,
  I_NAME   VARCHAR2(24 BYTE),
  I_PRICE  NUMBER(5,2),
  I_DATA   VARCHAR2(50 BYTE)
)
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;


CREATE UNIQUE INDEX TTAC.ITEM_I1 ON TTAC.ITEM
(I_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.ITEM_I1
  MONITORING USAGE;
