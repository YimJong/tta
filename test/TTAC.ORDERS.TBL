DROP TABLE TTAC.ORDERS CASCADE CONSTRAINTS;

CREATE TABLE TTAC.ORDERS
(
  O_ID          NUMBER,
  O_W_ID        NUMBER,
  O_D_ID        NUMBER,
  O_C_ID        NUMBER,
  O_CARRIER_ID  NUMBER,
  O_OL_CNT      NUMBER,
  O_ALL_LOCAL   NUMBER,
  O_ENTRY_D     DATE
)
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;


CREATE UNIQUE INDEX TTAC.ORDERS_I1 ON TTAC.ORDERS
(O_W_ID, O_D_ID, O_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.ORDERS_I1
  MONITORING USAGE;
