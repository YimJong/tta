DROP TABLE TTAC.STOCK CASCADE CONSTRAINTS;

CREATE TABLE TTAC.STOCK
(
  S_I_ID        NUMBER(6),
  S_W_ID        NUMBER(4),
  S_QUANTITY    NUMBER(6),
  S_DIST_01     CHAR(24 BYTE),
  S_DIST_02     CHAR(24 BYTE),
  S_DIST_03     CHAR(24 BYTE),
  S_DIST_04     CHAR(24 BYTE),
  S_DIST_05     CHAR(24 BYTE),
  S_DIST_06     CHAR(24 BYTE),
  S_DIST_07     CHAR(24 BYTE),
  S_DIST_08     CHAR(24 BYTE),
  S_DIST_09     CHAR(24 BYTE),
  S_DIST_10     CHAR(24 BYTE),
  S_YTD         NUMBER(10),
  S_ORDER_CNT   NUMBER(6),
  S_REMOTE_CNT  NUMBER(6),
  S_DATA        VARCHAR2(50 BYTE)
)
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;


CREATE UNIQUE INDEX TTAC.STOCK_I1 ON TTAC.STOCK
(S_I_ID, S_W_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.STOCK_I1
  MONITORING USAGE;
