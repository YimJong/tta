DROP TABLE TTAC.UT_ANNOTATION_CACHE_INFO CASCADE CONSTRAINTS;

CREATE TABLE TTAC.UT_ANNOTATION_CACHE_INFO
(
  CACHE_ID      NUMBER(20)                      NOT NULL,
  OBJECT_OWNER  VARCHAR2(250 BYTE)              NOT NULL,
  OBJECT_NAME   VARCHAR2(250 BYTE)              NOT NULL,
  OBJECT_TYPE   VARCHAR2(250 BYTE)              NOT NULL,
  PARSE_TIME    TIMESTAMP(6)                    NOT NULL, 
  CONSTRAINT UT_ANNOTATION_CACHE_INFO_PK
  PRIMARY KEY
  (CACHE_ID)
  ENABLE VALIDATE
)
ORGANIZATION INDEX
PCTTHRESHOLD 50
TABLESPACE USERS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );


-- Index UT_ANNOTATION_CACHE_INFO_PK is created automatically by Oracle with index organized table UT_ANNOTATION_CACHE_INFO.
CREATE UNIQUE INDEX TTAC.UT_ANNOTATION_CACHE_INFO_UK ON TTAC.UT_ANNOTATION_CACHE_INFO
(OBJECT_OWNER, OBJECT_TYPE, OBJECT_NAME)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.UT_ANNOTATION_CACHE_INFO_UK
  MONITORING USAGE;

ALTER TABLE TTAC.UT_ANNOTATION_CACHE_INFO ADD (
  CONSTRAINT UT_ANNOTATION_CACHE_INFO_UK
  UNIQUE (OBJECT_OWNER, OBJECT_TYPE, OBJECT_NAME)
  USING INDEX TTAC.UT_ANNOTATION_CACHE_INFO_UK
  ENABLE VALIDATE);
