DROP TABLE TTAC.WAREHOUSE CASCADE CONSTRAINTS;

CREATE TABLE TTAC.WAREHOUSE
(
  W_ID        NUMBER(4),
  W_YTD       NUMBER(12,2),
  W_TAX       NUMBER(4,4),
  W_NAME      VARCHAR2(10 BYTE),
  W_STREET_1  VARCHAR2(20 BYTE),
  W_STREET_2  VARCHAR2(20 BYTE),
  W_CITY      VARCHAR2(20 BYTE),
  W_STATE     CHAR(2 BYTE),
  W_ZIP       CHAR(9 BYTE)
)
TABLESPACE USERS
PCTFREE    99
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;


CREATE UNIQUE INDEX TTAC.WAREHOUSE_I1 ON TTAC.WAREHOUSE
(W_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );

ALTER INDEX TTAC.WAREHOUSE_I1
  MONITORING USAGE;
